{%extends "SKCMSAdminBundle::base.html.twig"%}

{% block stylesheets %}
    <link href="//cdnjs.cloudflare.com/ajax/libs/select2/4.0.0-rc.2/css/select2.min.css" rel="stylesheet" />
    
        {% stylesheets 'bundles/skcmsadmin/css/*.css' filter="cssrewrite" %}
{#            <script type="text/javascript" src="{{ asset_url }}"></script>#}
            <link href="{{ asset_url }}" rel="stylesheet">
        {% endstylesheets %}
{#	<!-- The styles -->#}
{#	<link id="bs-css" href="css/bootstrap-cerulean.css" rel="stylesheet">#}
	<style type="text/css">
	  body {
		padding-bottom: 40px;
	  }
	  .sidebar-nav {
		padding: 9px 0;
	  }
	</style>
        <script src="{{asset('bundles/skcmsadmin/js/jquery-1.11.2.min.js')}}"></script>

{% endblock%}

{% block body %}
        
{#{{ render(controller('SKCMSAdminBundle:Admin:topNav', { })) }}#}
<div class="container-fluid" id="main-admin">
    <div class="row">
        
        {{ render(controller('SKCMSAdminBundle:Admin:leftNav', { })) }}
{#        {%include 'SKCMSAdminBundle:Element/Nav:left-bar.html.twig' %} #}
        <div id="content" class="col-md-10 col-lg-10 col-xs-12 col-sm-7">
            
            {% for label, flashes in app.session.flashbag.all %}
                {% for flash in flashes %}
                <div class="alert alert-warning alert-dismissible" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <strong>{{flash}}
                </div>
                {% endfor %}
            {% endfor %}
           {# <div>
                <ul class="breadcrumb">
                    <li>
                        <a href="#">Home</a> <span class="divider">/</span>
                    </li>
                    <li>
                        <a href="#">Dashboard</a>
                    </li>
                </ul>
            </div>#}
        {%block adminContent%}{%endblock%}
        </div>
    </div>

        <hr>

<div class="modal fade" id="myModal" role="dialog">
    
    <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="myModalLabel">Modal title</h4>
          </div>
          <div class="modal-body">
            <p>...</p>
          </div>
          <div class="modal-footer">
            <a href="#" class="btn" data-dismiss="modal">Close</a>
            <a href="#" class="btn btn-primary">Save changes</a>
          </div>
        </div>
    </div>
    
    
    
    
</div>

<footer class="row">
    <p class="col-md-12 col-lg-12 col-sm-12 col-xs-12">&copy; <a href="http://solid-kiss.be" target="_blank"><strong>SOLID</strong>-KISS</a> {{"now"|date('Y')}}</p>
{#    <p class="pull-right">Powered by: <a href="http://usman.it/free-responsive-admin-template">Charisma</a></p>#}
</footer>

</div><!--/.fluid-container-->

{% endblock%}

        

